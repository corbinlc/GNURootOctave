<resources>
    <string name="app_name">GNURoot Octave</string>
    <string name="launch_preference">Would you like to launch octave in a graphical or non-graphical terminal?</string>
    <string name="term_preference">Non-graphical</string>
    <string name="xterm_preference">Graphical</string>
    <string name="install_button_text">Install/Update</string>
    <string name="install_textview_text">Will Install/Update Octave when the button is pressed.  You may also select additional packages to Install/Update at the same time.</string>
    <string-array name="package_disp_array">
        <item>gnuplot (required for plotting)</item>
        <item>Control Package</item>
        <item>Financial Package</item>
        <item>IO Package</item>
        <item>Missing Functions Package</item>
        <item>ODE Package</item>
        <item>Optim Package</item>       
        <item>Signal Package</item>
        <item>Specfun Package</item>
        <item>Statistics Package</item>
        <item>Symbolic Package</item>
        <item>Image Package</item>
    </string-array>
    <string-array name="package_name_array">
        <item>gnuplot-nox</item>
        <item>octave-control</item>
        <item>octave-financial</item>
        <item>octave-io</item>
        <item>octave-missing-functions</item>
        <item>octave-odepkg</item>
        <item>octave-optim</item>       
        <item>octave-signal</item>
        <item>octave-specfun</item>
        <item>octave-statistics</item>
        <item>octave-symbolic</item>
        <item>octave-image</item>
    </string-array>
    <string name="launch_button_text">Run Octave</string>
    <string name="launch_textview_text">Pressing the button below will run Octave in a new terminal via GNURoot Debian.</string>
    <string name="launch_x_button_text">Run X Octave</string>
    <string name="launch_x_textview_text">Pressing the button below will run Octave in a new Xterm via GNURoot Debian.</string>
</resources>
